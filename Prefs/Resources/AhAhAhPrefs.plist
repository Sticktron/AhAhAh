<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>title</key>
	<string>Ah!Ah!Ah!</string>
	<key>items</key>
	<array>
		
		<!--+ HEADER +-->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>headerCellClass</key>
			<string>AhAhAhLogoCell</string>
		</dict>
		
		<!-- Enabled Switch (YES) -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>cellClass</key>
			<string>AhAhAhSwitchCell</string>
			<key>label</key>
			<string>Enabled</string>
			<key>key</key>
			<string>Enabled</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.sticktron.ahahah</string>
			<key>PostNotification</key>
			<string>com.sticktron.ahahah.prefschanged</string>
		</dict>
		
		
		<!--+ APPEARANCE +-->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Appearance</string>
			<key>footerText</key>
			<string>Default is the mode defined by the selected Theme, or else AspectFit.</string>
		</dict>
		
		<!-- Theme -->
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>cellClass</key>
			<string>AhAhAhThemeLinkCell</string>
			<key>detail</key>
			<string>AhAhAhPrefsThemeController</string>
			<key>label</key>
			<string>Theme</string>
		</dict>
		
		<!-- Content Mode ("Default") -->
		<dict>
			<key>cell</key>
			<string>PSLinkListCell</string>
			<key>label</key>
			<string>Resize Mode</string>
			<key>detail</key>
			<string>AhAhAhListItemsController</string>
			<key>validTitles</key>
			<array>
				<string>Default</string>
				<string>AspectFit</string>
				<string>AspectFill</string>
				<string>Fill</string>
			</array>
			<key>validValues</key>
			<array>
				<string>Default</string>
				<string>AspectFit</string>
				<string>AspectFill</string>
				<string>Fill</string>
			</array>
			<key>key</key>
			<string>ContentMode</string>
			<key>default</key>
			<string>Default</string>
			<key>defaults</key>
			<string>com.sticktron.ahahah</string>
			<key>PostNotification</key>
			<string>com.sticktron.ahahah.prefschanged</string>
		</dict>
		
		
		<!--+ SOUND +-->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Sound</string>
		</dict>
		
		<!-- Force Volume (YES) -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>cellClass</key>
			<string>AhAhAhSwitchCell</string>
			<key>label</key>
			<string>Force Volume Level</string>
			<key>key</key>
			<string>ForceVolume</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.sticktron.ahahah</string>
			<key>PostNotification</key>
			<string>com.sticktron.ahahah.prefschanged</string>
		</dict>
		
		<!-- Volume Slider (75%) -->
		<dict>
			<key>cell</key>
			<string>PSSliderCell</string>
			<key>cellClass</key>
			<string>AhAhAhSliderCell</string>
			<key>label</key>
			<string>Volume</string>
			<key>min</key>
			<real>0</real>
			<key>max</key>
			<real>100</real>
			<key>showValue</key>
			<false/>
			<key>key</key>
			<string>VolumeLevel</string>
			<key>default</key>
			<real>75</real>
			<key>defaults</key>
			<string>com.sticktron.ahahah</string>
			<key>PostNotification</key>
			<string>com.sticktron.ahahah.prefschanged</string>
		</dict>
		
		
		<!--+ Max Failed Unlocks (2) +-->
 		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Trigger the alarm after this many failed attempts to unlock the device.</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSLinkListCell</string>
			<key>label</key>
			<string>Max Failed Unlocks</string>
			<key>detail</key>
			<string>AhAhAhListItemsController</string>
			<key>validTitles</key>
			<array>
				<string>1</string>
				<string>2</string>
				<string>3</string>
				<string>4</string>
				<string>5</string>
			</array>
			<key>validValues</key>
			<array>
				<integer>1</integer>
				<integer>2</integer>
				<integer>3</integer>
				<integer>4</integer>
				<integer>5</integer>
			</array>
			<key>staticTextMessage</key>
			<string>* Default is 2</string>
			<key>key</key>
			<string>MaxFailures</string>
			<key>default</key>
			<integer>2</integer>
			<key>defaults</key>
			<string>com.sticktron.ahahah</string>
			<key>PostNotification</key>
			<string>com.sticktron.ahahah.prefschanged</string>
		</dict>
		
		
		<!--+ Ignore Bio Failures (NO) +-->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Don't count Touch ID mis-matches as attempts to unlock.</string>
		</dict>
		
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>cellClass</key>
			<string>AhAhAhSwitchCell</string>
			<key>label</key>
			<string>Ignore Touch ID Fails</string>
			<key>id</key>
			<string>IgnoreBioFailure</string>
			<key>key</key>
			<string>IgnoreBioFailure</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.sticktron.ahahah</string>
			<key>PostNotification</key>
			<string>com.sticktron.ahahah.prefschanged</string>
		</dict>
		
		
		<!--+ Disable Lock Button (YES) +-->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Don't let the Lock Button stop the alarm. The alarm will stop by itself after 30 seconds.</string>
		</dict>
		
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>cellClass</key>
			<string>AhAhAhSwitchCell</string>
			<key>label</key>
			<string>Disable Lock Button</string>
			<key>key</key>
			<string>DisableLockButton</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.sticktron.ahahah</string>
			<key>PostNotification</key>
			<string>com.sticktron.ahahah.prefschanged</string>
		</dict>
		
		
		<!--+ GET SUPPORT +-->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Get Support</string>
		</dict>
		
		<!-- Email -->
		<dict>
			<key>action</key>
			<string>openEmail</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>cellClass</key>
			<string>AhAhAhButtonCell</string>
			<key>icon</key>
			<string>Email.png</string>
			<key>label</key>
			<string>Send an Email</string>
		</dict>
		
		<!-- Twitter -->
		<dict>
			<key>action</key>
			<string>openTwitter</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>cellClass</key>
			<string>AhAhAhButtonCell</string>
			<key>icon</key>
			<string>Twitter.png</string>
			<key>label</key>
			<string>Send a Tweet</string>
		</dict>
		
		<!-- Reddit -->
		<dict>
			<key>action</key>
			<string>openReddit</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>cellClass</key>
			<string>AhAhAhButtonCell</string>
			<key>icon</key>
			<string>Reddit.png</string>
			<key>label</key>
			<string>Get help at /r/jailbreak</string>
		</dict>
		
		
		<!-- GIVE SUPPORT -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Give Support</string>
		</dict>
		
		<!-- GitHub -->
		<dict>
			<key>action</key>
			<string>openGitHub</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>cellClass</key>
			<string>AhAhAhButtonCell</string>
			<key>icon</key>
			<string>GitHub.png</string>
			<key>label</key>
			<string>Contribute to Source Code</string>
		</dict>
		
		<!-- PayPal -->
		<dict>
			<key>action</key>
			<string>openPayPal</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>cellClass</key>
			<string>AhAhAhButtonCell</string>
			<key>icon</key>
			<string>PayPal.png</string>
			<key>label</key>
			<string>Contribute via PayPal</string>
		</dict>
		
		
		<!--+ CREDITS +-->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerAlignment</key>
			<string>1</string>
			<key>footerText</key>
			<string>
Copyright Â© 2014-2016 Sticktron</string>
		</dict>
		
		<!-- Credits -->
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>AhAhAhPrefsCreditsController</string>
			<key>label</key>
			<string>Credits</string>
		</dict>
		
	</array>
</dict>
</plist>
